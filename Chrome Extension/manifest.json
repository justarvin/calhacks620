{
  "manifest_version": 2,

  "name": "CalHakcs 620",
  "description": "Extension that allows for fingerprint login",
  "version": "1.0",

  "background": {
    "page": "background.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
